              #c2 and o2 are for constant t value traces
              #c3 and o3 are for constant s value traces
              c1="gray"; c2="black"; c3="black"; o1=0.2; o2=0.3; o3=0.3;
              plot99=parametric_plot3d(param,(s,a1,a2),(t,b1,b2),color=c1,opacity=o1,aspect_ratio=(1,1,0.4))
              for i in range(n+1):
                sv=a1+i/n*(a2-a1)
                plot99+=parametric_plot3d(param(sv,t),(t,b1,b2),color=c2,opacity=o2)
              for i in range(m+1):
                tv=b1+i/m*(b2-b1)
                plot99+=parametric_plot3d(param(s,tv),(s,a1,a2),color=c3,opacity=o3)
              fx=diff(f,x)
              fy=diff(f,y)
              fv=f(1.75,a)
              fxv=fx(1.75,a)
              fyv=fy(1.75,a)
              fxy=diff(fx,y)
